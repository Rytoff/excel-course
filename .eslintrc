{
  "parser": "babel-eslint",
  "env": {
    "es6": true,
    "browser": true,
    "node": true,
    "jest/globals": true
  },
  "extends": ["eslint:recommended", "google"],
  "plugins": ["jest"],
  "rules": {
    "semi": "off",
    "arrow-parens": "off",
    "comma-dangle": "off",
    "require-jsdoc": "off",
    "linebreak-style": "off",
    "eol-last": "off",
    "indent": "off",
    "object-curly-spacing": 0,
    "operator-linebreak": "off",
    "no-unused-vars": "off"
  }
}